
// path to your public key
variable "public_key_path" {
  default = ""
}

// path to your private key
variable "private_key_path" {
  default = ""
}

// your machine's public ip for ssh connection (format is "your_ip/32") 
// (you can find with "$ curl ifconfig.me"
variable "LOCAL_MACHINE_IP" {
  default = "/32"
}



